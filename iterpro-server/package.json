{
	"name": "iterpro-server",
	"version": "1.85.3",
	"description": "Core back-end for Iterpro football intelligence platform",
	"main": "./app.js",
	"engines": {
		"node": ">=14.0.0"
	},
	"scripts": {
		"commit": "cz",
		"debug": " export TZ=UTC; APP_ENV=development node --trace-deprecation --nolazy --inspect=9229 --max-old-space-size=4096 app.js",
		"debug:staging": " export TZ=UTC; APP_ENV=staging node --trace-deprecation --nolazy --inspect=9229 --max-old-space-size=4096 app.js",
		"debug:production": " export TZ=UTC; APP_ENV=production node --trace-deprecation --nolazy --inspect=9229 --max-old-space-size=4096 app.js",
		"start": "node --max-old-space-size=4096 app.js",
		"start:local": "export TZ=UTC; APP_ENV=development node app.js",
		"start:staging": "export TZ=UTC;APP_ENV=staging node app.js",
		"start:production": "export TZ=UTC;APP_ENV=production node app.js",
		"start:test": "export TZ=UTC;APP_ENV=test node app.js",
		"sdk": " APP_ENV=development LANG=en ../loopback-sdk-builder/bin/lb-sdk server/server.js ../iterpro-frontend/libs/shared/data-access/sdk/src/lib",
		"swagger-codegen": "node ./utils/swagger-codegen.js",
		"execute-test": "mongorestore --drop  --uri mongodb://localhost:27017/iterpro_test ./test/db &&  NODE_ENV=test ./node_modules/mocha/bin/mocha --file ./test/setup-env.js  --exit",
		"post-test": "npm run lint && nsp check",
		"prepare": "husky install",
		"format:check": "prettier --check .",
		"format:write": "prettier --write .",
		"lint:check": "eslint .",
		"lint:fix": "eslint --fix ."
	},
	"dependencies": {
		"@azure/cosmos": "^3.17.3",
		"@azure/storage-blob": "^12.0.0",
		"@azure/storage-queue": "^12.11.0",
		"@jsreport/nodejs-client": "^4.0.2",
		"@sendgrid/mail": "^8.0.0",
		"applicationinsights": "^2.3.3",
		"async-redis": "^2.0.0",
		"axios": "^1.6.4",
		"axios-retry": "^3.1.1",
		"azure-arm-mediaservices": "^6.0.0",
		"chai-http": "^4.3.0",
		"compression": "^1.7.1",
		"cors": "^2.8.4",
		"dayjs": "^1.11.7",
		"dotenv": "^7.0.0",
		"ejs": "^3.1.10",
		"express-package-json": "^1.0.2",
		"helmet": "^4.4.1",
		"html-to-text": "^9.0.0",
		"jsonwebtoken": "^9.0.2",
		"lodash": "^4.17.21",
		"loopback": "^3.26.0",
		"loopback-boot": "2.28.0",
		"loopback-cascade-delete-mixin": "^2.0.0",
		"loopback-component-explorer": "^6.5.1",
		"loopback-connector-mongodb": "^4.2.0",
		"loopback-datasource-juggler": "3.36.1",
		"loopback-ds-timestamp-mixin": "3.4.1",
		"mathjs": "^13.1.1",
		"moment": "^2.30.1",
		"moment-duration-format": "^2.3.2",
		"moment-range": "^4.0.2",
		"moment-timezone": "^0.5.34",
		"multer": "^1.4.4",
		"qrcode": "^1.4.2",
		"redis": "^2.8.0",
		"redis-delete-pattern": "^0.1.0",
		"route-parser": "0.0.5",
		"serve-favicon": "^2.4.5",
		"socket.io": "^4.0.0",
		"speakeasy": "^2.0.0",
		"strong-error-handler": "^4.0.0",
		"uuid": "3.4.0"
	},
	"devDependencies": {
		"@mean-expert/loopback-sdk-builder": "2.2.0",
		"chai": "^4.2.0",
		"commitizen": "^4.2.5",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.32.0",
		"eslint-config-loopback": "^13.1.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-security": "^1.7.1",
		"husky": "^8.0.2",
		"loopback-component-visualizer": "^1.2.1",
		"mocha": "^10.7.3",
		"nock": "^10.0.6",
		"nsp": "^2.8.1",
		"prettier": "^2.7.1"
	},
	"private": true,
	"license": "UNLICENSED",
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
