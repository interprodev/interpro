{
	"restApiRoot": "/api",
	"host": "localhost",
	"port": 3000,
	"remoting": {
		"context": false,
		"rest": {
			"handleErrors": false,
			"normalizeHttpPath": false,
			"xml": false
		},
		"json": {
			"strict": false,
			"limit": "10mb"
		},
		"urlencoded": {
			"extended": true,
			"limit": "10mb"
		},
		"cors": false
	}
}
